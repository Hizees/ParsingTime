#!/bin/bash

#-----
# View a file from the last run
#-----
function v {
	file=$1
	last=`ls out | sort -nr | head -n 1`
	vim out/$last/$file
}

#-----
# Utilities
#-----
function r {
	make && ./run
}

function cmd {
	java -cp bin:etc/joda-time.jar:etc/scala-compiler.jar:etc/scala-library.jar:etc/jline.jar time.Time
}
